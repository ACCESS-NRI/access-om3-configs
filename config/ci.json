{
    "$schema": "https://github.com/ACCESS-NRI/schema/tree/main/au.org.access-nri/model/configuration/ci/2-0-0.json",
    "scheduled": {
        "default": {
            "markers": "repro or access_om3 or config"
        },
        "dev-MC_1deg_jra_ryf": {
            "markers": "repro or access_om3 or dev_config"
        },
        "dev-MCW_1deg_jra_ryf": {
            "markers": "repro or access_om3 or dev_config"
        },
        "release-MC_1deg_jra_ryf": { },
        "release-MCW_1deg_jra_ryf": { },
        "release-.*": {
            "model-config-tests-version": "0.1.0",
            "payu-version": "1.1.6"
        }
    },
    "reproducibility": {
        "default": {
            "markers": "repro and (not slow)"
        },
        "dev-MC_1deg_jra_ryf": {
            "markers": "repro"
        },
        "dev-MCW_1deg_jra_ryf": {
            "markers": "repro"
        },
        "release-MC_1deg_jra_ryf": {
            "markers": "repro"
        },
        "release-MCW_1deg_jra_ryf": {
            "markers": "repro"
        },
        "release-.*": {
            "model-config-tests-version": "0.1.0",
            "payu-version": "1.1.6"
        }
    },
    "qa": {
        "default": {
            "markers": "access_om3 or dev_config"
        },
        "release-.*": {
            "markers": "access_om3 or config",
            "model-config-tests-version": "0.1.0",
            "payu-version": "1.1.6"
        }
    },
    "default": {
        "model-config-tests-version": "main",
        "python-version": "3.11.0",
        "payu-version": "dev"
    }
